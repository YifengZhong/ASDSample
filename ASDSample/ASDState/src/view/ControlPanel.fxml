<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ControlPanelCtl">
   <children>
      <GridPane id="scale" prefHeight="246.0" prefWidth="355.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button id="left" mnemonicParsing="false" onAction="#onLeft" text="Left" GridPane.rowIndex="1" />
            <Button id="right" mnemonicParsing="false" onAction="#onRight" text="Right" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button id="accel" mnemonicParsing="false" onAction="#onAccel" text="Accel" GridPane.rowIndex="2" />
            <Button id="brake" mnemonicParsing="false" onAction="#onBrake" text="Brake" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <ComboBox id="roadState" fx:id="roadState" onAction="#onSelect" prefHeight="65.0" prefWidth="183.0" />
            <Label id="scale"  fx:id="scale" text="Scale" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
